	<e:Label text="{data.job}" horizontalCenter="0" size="30" y="80" visible="{data.showJob}"/>
			</e:Skin>
		</e:itemRendererSkinName>
		<e:layout>
			<e:TileLayout requestedColumnCount="4" orientation="rows" horizontalAlign="center" horizontalGap="0"  verticalGap="30"/>
		</e:layout>
		<e:ArrayCollection>
			<e:Array></e:Array>
		</e:ArrayCollection>
	</e:DataGroup>
	<e:Button id="btnCheck" label="" y="988" skinName="eui.Button$Common" horizontalCenter="-138"  icon="txt_check_png"/>
	<e:Button id="btnEnd" label="" y="988" skinName="eui.Button$Common" horizontalCenter="138"  icon="txt_end_png"/>
