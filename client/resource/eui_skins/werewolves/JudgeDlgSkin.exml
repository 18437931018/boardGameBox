<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="app.werewolves.JudgeDlgSkin" width="720" height="1280" xmlns:e="http://ns.egret.com/eui"
        xmlns:w="http://ns.egret.com/wing">
		<e:Image source="img_bg3_jpg" horizontalCenter="0"/>
	<e:Image source="test_jpg" y="0" horizontalCenter="0" visible="false"/>
